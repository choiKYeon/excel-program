{
  "name": "my-next-electron-app",
  "version": "0.1.0",
  "private": true,
  "main": "public/eletron.js",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "electron": "electron public/electron.js",
